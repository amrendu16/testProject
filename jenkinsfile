#!groovy

pipeline {
    agent any
    tools {
          maven 'MAVEN 3.3.9'
          jdk 'jdk8'
        }

    stages {
        stage("build") {
            steps {
                sh "mvn -version"
                sh "mvn clean install"
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}